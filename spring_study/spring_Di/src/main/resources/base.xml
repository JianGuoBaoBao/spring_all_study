<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="student" class="com.demo.Student">
        <property name="name" value="meepo"/>
        <property name="address" ref="address"/>
        <property name="bookname">
            <array>
                <value>红楼梦</value>
                <value>洗衣机</value>
                <value>水浒传</value>
                <value>三国演义</value>
            </array>
        </property>
        <property name="hobbys">
            <list>
                <value>唱</value>
                <value>跳</value>
                <value>rap</value>
                <value>篮球</value>
            </list>
        </property>
        <property name="map">
            <map>
               <entry key="身份证" value="12332321"></entry>
               <entry key="name" value="hhh"></entry>
            </map>
        </property>
        <property name="set">
            <set>
                <value>唱</value>
                <value>跳</value>
                <value>rap</value>
                <value>篮球</value>
            </set>
        </property>

<!--        <property name="wife">-->
<!--             <null/>-->
<!--        </property>-->

        <property name="info">
            <props>
               <prop key="学号">201999</prop>
                <prop key="性别">男</prop>
                <prop key="姓名">小明</prop>
                <prop key="password">123456</prop>
            </props>
        </property>

    </bean>
    <bean id="address" class="com.demo.Address">
        <property name="address" value="上海"/>
    </bean>
</beans>